<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/i1.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>PAGE_1</from-outcome>
			<to-view-id>/i2.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>i3</from-outcome>
			<to-view-id>/i3.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>sv</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.jsflabbing.text.mymessages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
</faces-config>
